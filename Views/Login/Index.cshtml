@using SeriesMoviesProjectver2.Models.Entity
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="zxx">

<head>

    <title>Meetup Signin Form Flat Responsive Widget Template : W3layouts</title>

    <!-- Meta-Tags -->
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">

    <!-- style CSS -->
    <link rel="stylesheet" href="~/Pages/Login/starter/css/style.css" type="text/css" media="all">

    <!-- fontawesome -->
    <link rel="stylesheet" href="~/Pages/Login/starter/css/font-awesome.min.css" type="text/css" media="all">

    <!-- google fonts -->
    <link href="//fonts.googleapis.com/css?family=Mukta:300,400,500" rel="stylesheet">

</head>

<body>

    <section class="main">

        <div class="bottom-grid">
            <div class="logo">
                <h1> <a href="index.html">Sitemize Hoşgeldiniz  </a></h1>
            </div>
        </div>
        <div class="content-w3ls">
            <div class="text-center icon">
                <span class="fa fa-meetup"></span>
            </div>

            <div class="content-bottom">
                <form action="#" method="post">
                    <div class="field-group">
                        <span class="fa fa-user" aria-hidden="true"></span>
                        <div class="wthree-field">
                            <input name="Email" id="text1" type="text" value="" placeholder="Email Adresiniz" required>
                        </div>
                    </div>
                    <div class="field-group">
                        <span class="fa fa-lock" aria-hidden="true"></span>
                        <div class="wthree-field">
                            <input name="Password" id="myInput" type="Password" placeholder="Şifreniz">
                        </div>
                    </div>
                    <div class="wthree-field">
                        <button type="submit" class="btn">Giriş Yap</button>
                    </div>
                   
                   
                </form>
            </div>
        </div>
        <div class="copyright">
            <p>© 2021 Login Page | Design by Doğukan Kazan</p>
        </div>
    </section>

</body>
<!-- //Body -->

</html>

